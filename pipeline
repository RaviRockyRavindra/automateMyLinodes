pipeline{
agent any
tools{
    git "git"
}

stages{

stage('git clone')
{
steps{
git branch:'main',url:'https://github.com/RaviRockyRavindra/automateMyLinodes.git'
}

}

stage('execute the ansible playbook on controlled machines')
  {  
    steps{
sh 'ansible-playbook -i hosts playbookCreateFile.yml' 
     }
  }

}
}
